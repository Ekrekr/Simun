{
  "name": "simun",
  "version": "1.0.0",
  "description": "A website for experimenting with agent sharing, client and server side.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "browserify": "^16.2.3",
    "cookie-parser": "^1.4.4",
    "css": "^2.2.4",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.1",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "mssql": "^4.3.2",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "password-validator": "^4.1.1",
    "pidusage": "^2.0.17",
    "popper.js": "^1.15.0",
    "popups": "^1.1.3",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "redis": "^2.8.0",
    "req-flash": "0.0.3",
    "request": "^2.88.0",
    "sass": "^1.17.4",
    "sqlite3": "^4.0.6",
    "tedious": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "standard": {
      "env": [
        "mocha"
      ]
    }
  },
  "scripts": {
    "test": "standard --fix 'models/**/*.js' 'controller/*.js' 'tests/**/*.js'; mocha './{,!(node_modules)}/*.tests.js'",
    "open-website": "open http://localhost:8080",
    "start-server": "nodemon app",
    "compile-css": "sass --watch models/public/style:public/style",
    "compile-scripts": "concurrently \"watchify models/public/scripts/inbox.js -o public/scripts/inbox.js\" \"watchify models/public/scripts/tools.js -o public/scripts/tools.js\" \"watchify models/public/scripts/home.js -o public/scripts/home.js\" \"watchify models/public/scripts/outbox.js -o public/scripts/outbox.js\"",
    "dev": "npm run open-website; concurrently --kill-others \"npm run compile-css\" \"npm run compile-scripts\" \"npm run start-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ekrekr/Simon.git"
  },
  "author": "Elias Kassell Raymond and James Adams",
  "bugs": {
    "url": "https://github.com/Ekrekr/Simon/issues"
  },
  "homepage": "http://localhost:8080/"
}
