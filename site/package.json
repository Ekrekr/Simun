{
  "name": "simun",
  "version": "1.0.0",
  "description": "A website for experimenting with agent sharing, client and server side.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "browserify": "^16.2.3",
    "browserify-directory": "^1.0.2",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cookies": "^0.7.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "css": "^2.2.4",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "minimatch": "^3.0.4",
    "mssql": "^4.3.2",
    "mysql": "^2.16.0",
    "node-daemon": "0.0.15",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "popper.js": "^1.15.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "sass": "^1.17.4",
    "sql": "^0.78.0",
    "sqlite3": "^4.0.6",
    "tedious": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "standard": {
      "env": [
        "mocha"
      ]
    }
  },
  "scripts": {
    "tests": "standard --fix 'models/**/*.js' 'server/**/*.js' 'tests/**/*.js'; mocha './{,!(node_modules)}/*.tests.js'",
    "open-website": "open http://localhost:7000",
    "start-server": "nodemon server/server",
    "compile-css": "sass --watch models/public/style:public/style",
    "compile-html": "pug --watch models/public -o public",
    "compile-scripts": "concurrently \"watchify models/public/scripts/receive.js -o public/scripts/receive.js\" \"watchify models/public/scripts/tools.js -o public/scripts/tools.js\" \"watchify models/public/scripts/index.js -o public/scripts/index.js\"",
    "dev": "npm run open-website; concurrently --kill-others \"npm run compile-css\" \"npm run compile-scripts\" \"npm run start-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ekrekr/Simon.git"
  },
  "author": "Elias Kassell Raymond and James Adams",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Ekrekr/Simon/issues"
  },
  "homepage": "http://localhost:7000/"
}
